<includes>
	<include name="MediaThumbViewMediumPreview">
		<control type="group">
			<visible>Control.IsVisible(51)</visible>
			<control type="button" id="99">
				<texturefocus>-</texturefocus>
				<onfocus>SetFocus(61)</onfocus>
				<visible>IntegerGreaterThan(Container(51).NumPages,1)</visible>
			</control>
			<control type="button" id="99">
				<texturefocus>-</texturefocus>
				<onfocus>SetFocus(9111)</onfocus>
				<visible>!IntegerGreaterThan(Container(51).NumPages,1)</visible>
			</control>
			<control type="panel" id="51">
				<posx>50</posx>
				<posy>112</posy>
				<width>784</width>
				<height>588</height>
				<onleft>ActivateWindow(51)</onleft>
				<onright>99</onright>
				<onup>51</onup>
				<ondown>51</ondown>
				<scrolltime>200</scrolltime>
				<viewtype label="536">Medium</viewtype>
				<pagecontrol>61</pagecontrol>
				<orientation>vertical</orientation>
				<preloaditems>2</preloaditems>
				<itemlayout height="196" width="196">
					<control type="image">
						<posx>2</posx>
						<posy>2</posy>
						<width>192</width>
						<height>192</height>
						<texture>frame_device_sq.png</texture>
					</control>
					<control type="image">
						<posx>12</posx>
						<posy>12</posy>
						<width>172</width>
						<height>172</height>
						<info>ListItem.Thumb</info>
						<bordersize>4</bordersize>
						<bordertexture>info_cover_border.png</bordertexture>
						<visible>!Container.Content(Episodes) + !Container.Content(Seasons) + !Container.Content(Artists) + !Container.Content(Songs) + !SubString(Container.FolderPath,addons) + !IsEmpty(ListItem.Thumb)</visible>
					</control>
					<control type="image">
						<posx>12</posx>
						<posy>12</posy>
						<width>172</width>
						<height>118</height>
						<info>ListItem.Thumb</info>
						<bordersize>4</bordersize>
						<bordertexture>info_cover_border.png</bordertexture>
						<visible>[Container.Content(Episodes) | Container.Content(Seasons) | Container.Content(Artists) | Container.Content(Songs)] + !IsEmpty(ListItem.Thumb)</visible>
					</control>
					<control type="image">
						<posx>12</posx>
						<posy>12</posy>
						<width>172</width>
						<height>172</height>
						<texture background="true" fallback="DefaultAddon.png">$INFO[ListItem.Thumb]</texture>
						<bordersize>4</bordersize>
						<bordertexture>info_cover_border.png</bordertexture>
						<visible>SubString(Container.FolderPath,addons) + !IsEmpty(ListItem.Thumb)</visible>
					</control>
					<control type="image">
						<posx>12</posx>
						<posy>12</posy>
						<width>172</width>
						<height>118</height>
						<info>ListItem.ActualIcon</info>
						<visible>IsEmpty(ListItem.Thumb)</visible>
					</control>
					<control type="image">
						<posx>164</posx>
						<posy>162</posy>
						<width>24</width>
						<height>24</height>
						<info>ListItem.Overlay</info>
						<visible>Skin.HasSetting(WatchedStatus)</visible>
					</control>
					<control type="label">
						<posx>15</posx>
						<posy>126</posy>
						<width>166</width>
						<height>60</height>
						<font>font21sq</font>
						<aligny>center</aligny>
						<wrapmultiline>true</wrapmultiline>
						<label>$LOCALIZE[20359] $INFO[ListItem.Episode]</label>
						<visible>Container.Content(Episodes) + !IsEmpty(ListItem.Thumb)</visible>
						<textcolor>white</textcolor>
					</control>
					<control type="label">
						<posx>15</posx>
						<posy>126</posy>
						<width>166</width>
						<height>60</height>
						<font>font21sq</font>
						<aligny>center</aligny>
						<wrapmultiline>true</wrapmultiline>
						<info>ListItem.Label</info>
						<visible>Container.Content(Seasons) | Container.Content(Artists) | IsEmpty(ListItem.Thumb)</visible>
						<textcolor>white</textcolor>
					</control>
					<control type="label">
						<posx>15</posx>
						<posy>126</posy>
						<width>166</width>
						<height>60</height>
						<font>font21sq</font>
						<aligny>center</aligny>
						<wrapmultiline>true</wrapmultiline>
						<label>$INFO[ListItem.TrackNumber,,. ]$INFO[ListItem.Title]</label>
						<visible>Container.Content(Songs) + !IsEmpty(ListItem.Thumb)</visible>
						<textcolor>white</textcolor>
					</control>
				</itemlayout>
				<focusedlayout height="196" width="196">
					<control type="image">
						<posx>2</posx>
						<posy>2</posy>
						<width>192</width>
						<height>192</height>
						<texture>frame_device_sq.png</texture>
					</control>
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>196</width>
						<height>196</height>
						<texture>frame_device_sq_white.png</texture>
						<animation type="Focus">
							<effect type="fade" start="60" time="120"/>
							<effect type="zoom" start="97,97" center="auto" time="120" delay="60"/>
						</animation>
					</control>
					<control type="image">
						<posx>12</posx>
						<posy>12</posy>
						<width>172</width>
						<height>172</height>
						<info>ListItem.Thumb</info>
						<bordersize>4</bordersize>
						<bordertexture>info_cover_border.png</bordertexture>
						<visible>!Container.Content(Episodes) + !Container.Content(Seasons) + !Container.Content(Artists) + !Container.Content(Songs) + !SubString(Container.FolderPath,addons) + !IsEmpty(ListItem.Thumb)</visible>
					</control>
					<control type="image">
						<posx>12</posx>
						<posy>12</posy>
						<width>172</width>
						<height>118</height>
						<info>ListItem.Thumb</info>
						<bordersize>4</bordersize>
						<bordertexture>info_cover_border.png</bordertexture>
						<visible>[Container.Content(Episodes) | Container.Content(Seasons) | Container.Content(Artists) | Container.Content(Songs)] + !IsEmpty(ListItem.Thumb)</visible>
					</control>
					<control type="image">
						<posx>12</posx>
						<posy>12</posy>
						<width>172</width>
						<height>172</height>
						<texture background="true" fallback="DefaultAddon.png">$INFO[ListItem.Thumb]</texture>
						<bordersize>4</bordersize>
						<bordertexture>info_cover_border.png</bordertexture>
						<visible>SubString(Container.FolderPath,addons) + !IsEmpty(ListItem.Thumb)</visible>
					</control>
					<control type="image">
						<posx>12</posx>
						<posy>12</posy>
						<width>172</width>
						<height>118</height>
						<info>ListItem.ActualIcon</info>
						<visible>IsEmpty(ListItem.Thumb)</visible>
					</control>
					<control type="image">
						<posx>164</posx>
						<posy>162</posy>
						<width>24</width>
						<height>24</height>
						<info>ListItem.Overlay</info>
						<visible>Skin.HasSetting(WatchedStatus)</visible>
					</control>
					<control type="label">
						<posx>15</posx>
						<posy>126</posy>
						<width>166</width>
						<height>60</height>
						<font>font21sq</font>
						<aligny>center</aligny>
						<wrapmultiline>true</wrapmultiline>
						<label>$LOCALIZE[20359] $INFO[ListItem.Episode]</label>
						<visible>Container.Content(Episodes) + !IsEmpty(ListItem.Thumb)</visible>
						<textcolor>black</textcolor>
					</control>
					<control type="label">
						<posx>15</posx>
						<posy>126</posy>
						<width>166</width>
						<height>60</height>
						<font>font21sq</font>
						<aligny>center</aligny>
						<wrapmultiline>true</wrapmultiline>
						<info>ListItem.Label</info>
						<visible>Container.Content(Seasons) | Container.Content(Artists) | IsEmpty(ListItem.Thumb)</visible>
						<textcolor>black</textcolor>
					</control>
					<control type="label">
						<posx>15</posx>
						<posy>126</posy>
						<width>166</width>
						<height>60</height>
						<font>font21sq</font>
						<aligny>center</aligny>
						<wrapmultiline>true</wrapmultiline>
						<label>$INFO[ListItem.TrackNumber,,. ]$INFO[ListItem.Title]</label>
						<visible>Container.Content(Songs) + !IsEmpty(ListItem.Thumb)</visible>
						<textcolor>black</textcolor>
					</control>
				</focusedlayout>
			</control>
			<control type="group">
				<visible>Container.Scrolling</visible>
				<animation effect="fade" time="100" delay="400">Visible</animation>
				<animation effect="fade" time="300">Hidden</animation>
				<posx>393</posx>
				<posy>356</posy>
				<width>100</width>
				<height>100</height>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>100</width>
					<height>100</height>
					<texture>scroll_background.png</texture>
				</control>
				<control type="label">
					<posx>0</posx>
					<posy>0</posy>
					<width>100</width>
					<height>100</height>
					<align>center</align>
					<aligny>center</aligny>
					<label>$INFO[ListItem.SortLetter]</label>
					<textcolor>white</textcolor>
					<font>font48b</font>
				</control>
			</control>
			<control type="scrollbar" id="61">
				<description>scroll bar indicator for lists</description>
				<posx>836</posx>
				<posy>112</posy>
				<width>12</width>
				<height>588</height>
				<onleft>50</onleft>
				<onright>SetFocus(9111)</onright>
				<texturesliderbackground border="0,8,0,8">scrollbar_vertical_bg.png</texturesliderbackground>
				<texturesliderbar border="4,8,4,8">scrollbar_vertical_nib.png</texturesliderbar>
				<texturesliderbarfocus border="4,8,4,8">scrollbar_vertical_nib_on.png</texturesliderbarfocus>
				<orientation>vertical</orientation>
				<showonepage>false</showonepage>
			</control>
		</control>
	</include>
</includes>