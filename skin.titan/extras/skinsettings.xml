<settings>
    <!-- home layout -->
    <setting id="HomeLayout" value="||SUBLEVEL||HomeLayout_titantiles" label="$LOCALIZE[31309] >" condition="" icon="special://skin/extras/viewthumbs/homelayout_1.jpg" description=""/>
    
    <setting id="HomeLayout_titantiles" value="1" label="$LOCALIZE[31033]" condition="" icon="special://skin/extras/viewthumbs/homelayout_1.jpg" description=""/>
    <setting id="HomeLayout_titantiles" value="2" label="$LOCALIZE[31031]" condition="" icon="special://skin/extras/viewthumbs/homelayout_2.jpg" description="Skin default"/>
