<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="songs">
    <name>Recommended Music</name>
    <match>all</match>
	<rule field="rating" operator="greaterthan">
        <value>4</value>
    </rule>
	<limit>20</limit>
    <order direction="ascending">random</order>
</smartplaylist>
