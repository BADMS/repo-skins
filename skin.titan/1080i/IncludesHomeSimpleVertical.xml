			<animation type="Conditional" condition="!Control.IsVisible(4444)">
				<effect type="fade" start="100" end="0" time="250" delay="0" />
			</animation>
			<animation type="Conditional" condition="IntegerGreaterThan(Container(300).Position,0) + !Skin.HasSetting(VerticalHomeTiles)">
