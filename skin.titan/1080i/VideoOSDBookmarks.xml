<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window>
    <defaultcontrol always="true">1</defaultcontrol>
    <controls>
	
	<control type="group">
			<posx>-660</posx>
			<posy>0</posy>
			<animation type="WindowOpen">
				<effect type="slide" start="0" end="660" time="250" tween="cubic" easing="inout" />
			</animation>
			<animation type="WindowClose">
				<effect type="slide" start="660" end="0" time="250" tween="cubic" easing="inout" />
			</animation>
			<!--Panel-->
		<control type="image">
			<posy>0</posy>
			<posx>0</posx>
			<width>485</width>
			<height>1080</height>
			<texture>colors/color_white.png</texture>
			<animation effect="fade" end="99" time="0" condition="true">Conditional</animation>
			<colordiffuse>$VAR[panel_color]</colordiffuse>
			<aspectratio>stretch</aspectratio>
		</control>
		
		
		<!-- header -->
        <control type="label">
            <posx>61</posx>
            <posy>10</posy>
            <width min="80" max="1100">auto</width>
            <height>100</height>
            <font>Bold45</font>
            <align>left</align>
            <textcolor>white</textcolor>
            <label>$LOCALIZE[298] ($INFO[Container(11).NumItems])</label>
			<visible>!Window.IsActive(DialogKaiToast.xml) + !Window.IsActive(DialogExtendedProgressBar.xml)</visible>
        </control>
            <!--Label Menu: -->
            <control type="image">
                <!--Seperator-->
                <posx>21</posx>
                <posy>160</posy>
                <width>458</width>
                <height>20</height>
                <align>left</align>
                <texture>common/menu-separator.png</texture>
            </control>
			<control type="image">
                <!--Seperator-->
                <posx>21</posx>
                <posy>980</posy>
                <width>458</width>
                <height>20</height>
                <align>left</align>
                <texture>common/menu-separator.png</texture>
            </control>
		
			<control type="list" id="11">
                <!--List ID=11-->
                <width>458</width>
                <posx>60</posx>
                <posy>176</posy>
				<onright>Dialog.Close(all,true)</onright>
				<onleft>Dialog.Close(all,true)</onleft>
                <height>600</height>
                <onup>1</onup>
                <ondown>1</ondown>
                <pagecontrol>-</pagecontrol>
                <scrolltime>0</scrolltime>
                <itemlayout width="380" height="195">
                    <control type="image">
                        <posx>5</posx>
						<posy>5</posy>
						<width>370</width>
						<height>180</height>
                        <texture>$INFO[ListItem.Icon]</texture>
                        <aspectratio>stretch</aspectratio>
                    </control>
                </itemlayout>
                <focusedlayout width="380" height="195">

					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>380</width>
						<height>190</height>
						<texture border="150,2,150,2" colordiffuse="$INFO[Skin.String(ButtonFocusColor)]">colors/color_white.png</texture>
						<aspectratio>stretch</aspectratio>
						<visible>Control.HasFocus(11)</visible>
					</control>

                    <control type="image">
                        <posx>5</posx>
						<posy>5</posy>
						<width>370</width>
						<height>180</height>
                        <texture>$INFO[ListItem.Icon]</texture>
                        <aspectratio>stretch</aspectratio>
                    </control>
					<control type="image">
						<!--Panel Ovelay-->
						<posx>5</posx>
						<posy>5</posy>
						<width>370</width>
						<height>180</height>
						<colordiffuse>ccFFFFFF</colordiffuse>
						<texture diffuse="home/home_matte2.png">home/home_overlay.png</texture>
						<visible>Control.HasFocus(11)</visible>
					</control>
					<control type="label">
					<!--Label-->
					<posx>0</posx>
					<posy>107</posy>
					<width>380</width>
					<height>100</height>
					<align>center</align>
					<label>$INFO[ListItem.Label]$INFO[ListItem.Label2, (,)]</label>
					<font>Light25</font>
					<textcolor>white</textcolor>
					<shadowcolor>black</shadowcolor>
					<visible>Control.HasFocus(11)</visible>
				</control>
                </focusedlayout>
            </control>
			
            <control type="grouplist" id="1">
                <!--Buttons-->
                <posx>65</posx>
                <posy>775</posy>
                <height>250</height>
                <width>400</width>
                <itemgap>5</itemgap>
                <orientation>vertical</orientation>
                <onup>11</onup>
                <onright>Dialog.Close(all,true)</onright>
                <control type="button" id="2">
                    <description>Add</description>
					<include>SubMenuButton</include>
                    <height>60</height>
                    <width>370</width>
					<label>15019</label>
                </control>
                <control type="button" id="3">
                    <description>Delete</description>
					<include>SubMenuButton</include>
                    <height>60</height>
                    <width>370</width>
					<label>117</label>
                </control>
				<control type="button" id="99">
                    <description>Close</description>
					<include>SubMenuButton</include>
                    <height>60</height>
                    <width>370</width>
					<label>222</label>
					<onclick>Dialog.Close(all,true)</onclick>
                </control>
            </control>
			
        </control>
	

    </controls>
</window>