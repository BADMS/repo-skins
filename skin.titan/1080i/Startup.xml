<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window>
<!--Splash Screen-->
	<include>defaultskinsettings</include>
	<onload>AlarmClock(loadhome,ReplaceWindow(Home),00:6,silent)</onload>
	<controls>
	<!-- splash screen -->
	<control type="group" id="6666">
	<include>DimensionsFullscreen</include>
		<!-- splashscreen background -->
        <control type="image">
            <include>DimensionsFullscreen</include>
			<include>backgroundfade</include>
            <texture background="true">$VAR[splashScreenImage]</texture>
            <fadetime>800</fadetime>
        </control>
		<include>Footer</include>
		<control type="label">
            <posx>80</posx>
            <posy>985</posy>
            <width>520</width>
            <align>left</align>
            <font>Reg41</font>
            <textcolor>white</textcolor>
            <label>20186</label>
        </control>
		<control type="label">
            <posx>1840</posx>
            <posy>985</posy>
            <width>520</width>
            <align>right</align>
            <font>Reg41</font>
            <textcolor>white</textcolor>
            <label>Titan MediaBrowser</label>
        </control>
	</control>
	</controls>
</window>