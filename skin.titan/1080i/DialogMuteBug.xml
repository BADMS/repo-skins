<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window id="113">
    <defaultcontrol always="true">901</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <controls>
        <control type="group">
            <animation type="WindowOpen">
                <effect type="slide" start="200,0" end="0,0" time="400" tween="cubic" easing="inout" delay="0" />
            </animation>
            <animation type="WindowClose">
                <effect type="slide" start="0,0" end="200,0" time="400" tween="cubic" easing="inout" delay="0" />
            </animation>
            <control type="image">
                <right>50</right>
                <top>30</top>
                <width>105</width>
                <height>105</height>
                <texture>dialogs/volume/back.png</texture>
                <aspectratio>stretch</aspectratio>
            </control>
            <control type="group">
                <control type="image">
                    <right>40</right>
					<top>20</top>
					<width>125</width>
					<height>125</height>
                    <texture>dialogs/volume/volume-muted.png</texture>
                </control>
            </control>
        </control>
    </controls>
</window>