<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window>
    <defaultcontrol always="true">10</defaultcontrol>
    <controls>
        <!--Dialog Overlay-->
        <include>DialogOverlayExtra</include>
        <!--Dialog Yes/No-->
        <control type="group">
            <include>animation_window_open_close</include>
			 <control type="image">
				<posx>00</posx>
				<posy>280</posy>
				<width>1920</width>
				<height>440</height>
				<texture>colors/color_white.png</texture>
				<animation effect="fade" end="95" time="0" condition="true">Conditional</animation>
				<colordiffuse>$VAR[panel_color]</colordiffuse>
			</control>
            <control type="group">
                <posx>590</posx>
                <posy>520</posy>
                <control type="button" id="11">
                    <description>Yes button</description>
                    <posx>71</posx>
                    <posy>65</posy>
                    <width>300</width>
                    <height>90</height>
                    <onleft>10</onleft>
                    <onright>10</onright>
					<label>107</label>
                    <font>Reg36</font>
                    <include>DialogButtonOK</include>
                </control>
                <control type="button" id="10">
                    <description>No button</description>
                    <posx>368</posx>
                    <posy>65</posy>
                    <width>300</width>
                    <height>90</height>
                    <onleft>11</onleft>
                    <onright>11</onright>
                    <font>Reg36</font>
                    <label>106</label>
                    <include>DialogButtonCancel</include>
                </control>
            </control>
            <control type="label" id="1">
                <description>Dialog Header</description>
                <posx>100</posx>
                <posy>320</posy>
                <width>1720</width>
                <align>center</align>
                <aligny>top</aligny>
                <include>DialogHeader_Alt</include>
                <!--Doubles Line-->
            </control>
			<control type="textbox" id="9">
                <description>Dialog Header</description>
                <posx>100</posx>
                <posy>370</posy>
                <width>1720</width>
				<height>150</height>
                <align>center</align>
                <aligny>top</aligny>
                <include>DialogHeader2</include>
                <!--Doubles Line-->
            </control>
           
        </control>
    </controls>
</window>