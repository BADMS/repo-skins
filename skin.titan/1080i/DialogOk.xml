<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window>
    <defaultcontrol always="true">10</defaultcontrol>
	
    <controls>
        <!--Dialog Overlay-->
        <include>DialogOverlayExtra</include>
        <!--Dialog OK-->
        <control type="group">
            <include>animation_window_open_close</include>
			 <control type="image">
				<posx>0</posx>
				<posy>320</posy>
				<width>1920</width>
				<height>440</height>
				<texture>colors/color_white.png</texture>
