					<width>1920</width>
					<height>22</height>
					<font>Reg36</font>
					<align>center</align>
					<textcolor>77FFFFFF</textcolor>
				</control>
				<control type="textbox" id="9">
					<description>Dialog Header</description>
					<width>1820</width>
					<height>150</height>
					<align>center</align>
