							<!--label>&lt;</label-->
							<posx>-4</posx>
							<height>85</height>
							<width>81</width>
							<texturefocus>osd/fullscreen/previous-fo.png</texturefocus>
							<texturenofocus>osd/fullscreen/previous-nofo.png</texturenofocus>
							<onclick>playercontrol(previous)</onclick>
							<onup>20</onup>
							<ondown>-</ondown>
							<onleft>11</onleft>
							<onright>3</onright>
					</control>
					<control type="togglebutton" id="3">
