							<posy>230r</posy>
							<width>270</width>
							<height>70</height>
							<font>Medium</font>
							<texturebg>osd/fullscreen/osdsettings.png</texturebg>
							<onup>500</onup>
							<ondown>500</ondown>
							<onleft>503</onleft>
							<onright>503</onright>
