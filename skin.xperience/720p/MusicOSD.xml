							<onup>20</onup>
							<ondown>-</ondown>
							<onleft>5</onleft>
							<onright>7</onright>
					</control>
					<control type="button" id="7">
							<description>Skip Forward</description>
							<!--label>&gt;</label-->
							<posx>374</posx>
							<height>85</height>
							<width>81</width>
							<texturefocus>osd/fullscreen/next-fo.png</texturefocus>
							<texturenofocus>osd/fullscreen/next-nofo.png</texturenofocus>
							<onclick>playercontrol(next)</onclick>
							<onup>20</onup>
							<ondown>-</ondown>
							<onleft>6</onleft>
							<onright>8</onright>
					</control>
					<control type="button" id="8">
							<description>Record</description>
							<posx>490</posx>
							<posy>20</posy>
							<height>50</height>
							<width>50</width>
							<texturefocus>osd/fullscreen/rec-fo.png</texturefocus>
							<texturenofocus>osd/fullscreen/rec-nofo.png</texturenofocus>
							<onclick>XBMC.PlayerControl(record)</onclick>
							<enable>Player.CanRecord</enable>
							<onleft>7</onleft>
							<onright>13</onright>
							<onup>20</onup>
							<ondown>-</ondown>
							<animation effect="fade" start="100" end="30" time="0" condition="!Player.CanRecord">Conditional</animation>
							<animation effect="slide" start="0" end="90" time="0" condition="!LastFM.RadioPlaying">Conditional</animation>
					</control>
					<control type="button" id="13">
							<description>Last.FM Button Love</description>
							<posx>535</posx>
							<posy>20</posy>
							<height>50</height>
							<width>50</width>
							<texturefocus>osd/fullscreen/lastfml-fo.png</texturefocus>
							<texturenofocus>osd/fullscreen/lastfml-nofo.png</texturenofocus>
