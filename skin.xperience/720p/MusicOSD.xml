							<!--label>&lt; &lt;</label-->
							<posx>72</posx>
							<height>85</height>
							<width>81</width>
							<texturefocus>osd/fullscreen/rewind-fo.png</texturefocus>
							<texturenofocus>osd/fullscreen/rewind-nofo.png</texturenofocus>
							<usealttexture>Player.Rewinding</usealttexture>
							<alttexturefocus>osd/fullscreen/ff-rr-blank.png</alttexturefocus>
							<alttexturenofocus>osd/fullscreen/ff-rr-blank.png</alttexturenofocus>
							<onclick>playercontrol(rewind)</onclick>
							<onup>20</onup>
							<ondown>-</ondown>
							<onleft>2</onleft>
							<onright>4</onright>
					</control>
					<control type="togglebutton" id="4">
