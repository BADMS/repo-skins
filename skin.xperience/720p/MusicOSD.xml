<window id="120">
		<defaultcontrol always="false">4</defaultcontrol>
		<allowoverlay>no</allowoverlay>
		<controls>
	<control type="group">
			<include>NowPlayingFullscreen</include>
			<!-- Main Part -->
			<control type="image">
				<!-- Back plate for now playing controls -->
				<posx>0</posx>
				<posy>153r</posy>
				<width>1280</width>
				<height>153</height>
				<texture>osd/fullscreen/osd-back.png</texture>
				<aspectratio>stretch</aspectratio>
			</control>
		<control type="group">
			<posx>200</posx>
			<posy>530</posy>
			<!-- +1 -->
			<control type="image">
					<posx>70</posx>
					<posy>30</posy>
					<width>60</width>
					<height>60</height>
					<texture fallback="OSD/nowplaying/nothumb.png" diffuse="osd/fullscreen/musiccover-diffuse.png">$INFO[musicplayer.offset(2).cover]</texture>
					<aspectratio>stretch</aspectratio>
					<fadetime>400</fadetime>
			</control>
			<control type="image">
					<posx>70</posx>
					<posy>90</posy>
					<width>60</width>
					<height>60</height>
					<texture fallback="OSD/nowplaying/nothumb.png" flipy="true" diffuse="osd/fullscreen/cover-ref-diffuse.png">$INFO[musicplayer.offset(2).cover]</texture>
					<aspectratio>stretch</aspectratio>
					<fadetime>400</fadetime>
			</control>
			<!-- -1 -->
			<control type="image">
					<posx>0</posx>
					<posy>20</posy>
					<width>80</width>
					<height>80</height>
					<texture fallback="OSD/nowplaying/nothumb.png" diffuse="osd/fullscreen/musiccover-diffuse.png">$INFO[musicplayer.offset(1).cover]</texture>
					<aspectratio>stretch</aspectratio>
					<fadetime>800</fadetime>
			</control>
			<control type="image">
					<posx>0</posx>
					<posy>100</posy>
					<width>80</width>
					<height>80</height>
					<texture fallback="OSD/nowplaying/nothumb.png" flipy="true" diffuse="osd/fullscreen/cover-ref-diffuse.png">$INFO[musicplayer.offset(1).cover]</texture>
					<aspectratio>stretch</aspectratio>
					<fadetime>800</fadetime>
			</control>
			<!-- current playing -->
			<control type="image">
					<posx>-80</posx>
					<posy>10</posy>
					<width>100</width>
					<height>100</height>
					<texture fallback="OSD/nowplaying/nothumb.png" diffuse="osd/fullscreen/musiccover-diffuse.png">$INFO[musicplayer.cover]</texture>
					<aspectratio>stretch</aspectratio>
					<fadetime>1200</fadetime>
			</control>
			<control type="image">
					<posx>-80</posx>
					<posy>110</posy>
					<width>100</width>
					<height>100</height>
					<texture fallback="OSD/nowplaying/nothumb.png" flipy="true" diffuse="osd/fullscreen/cover-ref-diffuse.png">$INFO[musicplayer.cover]</texture>
					<aspectratio>stretch</aspectratio>
					<fadetime>1200</fadetime>
			</control>
		</control>
			<control type="group">
