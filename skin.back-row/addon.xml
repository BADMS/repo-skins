<?xml version="1.0" encoding="UTF-8"?>
<addon
id="skin.back-row"
version="2.8.7" 
name="Back Row"
provider-name="Sharpe and DJDafreund">
<requires>
<import addon="xbmc.gui" version="2.11"/>
<import addon="script.recentlyadded" version="1.0.5"/>
<import addon="script.favourites" version="1.0.1"/> 
<import addon="script.logo-downloader" version="1.4.0"/> 
</requires>
<extension point="xbmc.gui.skin"
defaultresolution="PAL16x9" defaultresolutionwide="PAL16x9"
defaultthemename="textures.xbt" effectslowdown="0.75"
debugging="false"/>
<extension point="xbmc.addon.metadata">
<platform>all</platform>
<minversion app="xbmc">28000</minversion>
<summary>Back-Row skin by Sharpe and DJDafreund</summary>
<description>Back-Row has been largely built around ideas and suggestions from the XBMC community. Over the years it has become a highly polished theme that currently works very well for both SD and HD setups.</description>
<disclaimer>Should cause no issues</disclaimer>
</extension>
</addon>
