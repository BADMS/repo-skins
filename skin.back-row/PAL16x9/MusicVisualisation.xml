<window>
<id>2006</id>
<defaultcontrol>2</defaultcontrol>
<allowoverlay>yes</allowoverlay>
<controls>

<control type="group">
<visible>!Skin.HasSetting(Music Fanart Visualisation Toggle)</visible>

<control>
<description>visualisation</description>
<type>visualisation</type>
<id>2</id>
<posx>0</posx>
<posy>0</posy>
<width>720</width>
<height>576</height>
</control>

<!-- Codec & Viz Infos -->




<control>
<description>Preset Background Image</description>
<type>image</type>
<posx>-10</posx>
<posy>75</posy>
<width>740</width>
<height>65</height>
<texture>osd_menu_bg.png</texture>
<include>media-infopanel-effect</include>
<visible>Player.ShowCodec</visible>
<colordiffuse>99FFFFFF</colordiffuse>
</control>

<control>
<description>Codec Info</description>
<type>label</type>
<width>600</width>
<posx>75</posx>
<posy>85</posy>
<label>Audio | Codec: $INFO[musicplayer.Codec] - Bitrate: $INFO[musicplayer.Bitrate]kbps - Bits: $INFO[musicplayer.bitspersample]bit - Freq.: $INFO[musicplayer.Samplerate]kHz - Channels: $INFO[musicplayer.Channels]</label>
<align>left</align>
<font>font12</font>
<include>media-infopanel-effect</include>
<visible>Player.ShowCodec</visible>
</control>

<control>
<description>Presets Label</description>
<type>label</type>
<posx>75</posx>
<posy>110</posy>
<width>560</width>
<label>Visual | fps: $INFO[System.FPS] - </label>
<font>font12</font>
<align>left</align>
<include>media-infopanel-effect</include>
<visible>Player.ShowCodec</visible>
</control>

<control>
<description>Presets Label</description>
<type>label</type>
<posx>225</posx>
<posy>110</posy>
<width>420</width>
<label>$INFO[Visualisation.Name] - $INFO[Visualisation.Preset]</label>
<font>font12</font>
<align>left</align>
<include>media-infopanel-effect</include>
<visible>Player.ShowCodec</visible>
</control>

<control>
<description>Preset Locked Image</description>
<type>image</type>
<posx>210</posx>
<posy>111</posy>
<width>12</width>
<height>15</height>
<texture>arenaitem-private.png</texture>
<include>media-infopanel-effect</include>
<visible>Visualisation.Locked + Player.ShowCodec</visible>
</control>

</control>

<control type="group">
<visible>Skin.HasSetting(Music Fanart Visualisation Toggle)</visible>

<control type="multiimage">
<description>Back-up Slideshow</description>
<posx>0</posx>
<posy>0</posy>
<width>720</width>
<height>576</height>
<timeperimage>8000</timeperimage>
<fadetime>2000</fadetime>
<imagepath fallback="Now Playing Background.jpg" background="true">$INFO[Skin.String(Default Slideshow_Folder_Path)]</imagepath>
<randomize>true</randomize>
<aspectratio>scale</aspectratio>
<visible>Player.HasAudio</visible>
<visible>IsEmpty(MusicPlayer.Property(Fanart_Image))</visible>
<include>Slideshow Ken Burns Toggle</include>
<animation effect="fade" reversible="false" start="0" end="100" time="4000">WindowOpen</animation>
</control>


<control type="image">
<description>Fanart BG</description>
<texture>$INFO[MusicPlayer.Property(Fanart_Image)]</texture>
<posx>0</posx>
<posy>0</posy>
<width>720</width>
<height>576</height>
<aspectratio>stretch</aspectratio>
<fadetime>300</fadetime>
<animation effect="fade" reversible="false" start="0" end="100" time="4000">WindowOpen</animation>
<include>Slideshow Ken Burns Toggle</include>
</control>

</control>

<control type="group">
<visible>Skin.HasSetting(Music Fanart Visualisation Toggle)</visible>
<visible>Skin.HasSetting(Music Fanart Slideshow Toggle)</visible>

<control type="multiimage">
<description>Ronie's dirty hack for showing fanart slideshow instead of visualisation :)</description>
<posx>0</posx>
<posy>0</posy>
<width>720</width>
<height>576</height>
<timeperimage>8000</timeperimage>
<fadetime>2000</fadetime>
<imagepath background="true">$INFO[Skin.String(Music_Folder_Path)]$INFO[musicplayer.artist]/extrafanart/</imagepath>
<aspectratio>stretch</aspectratio>
<visible>Player.HasAudio</visible>
<include>Slideshow Ken Burns Toggle</include>
</control>

<control type="multiimage">
<description>Ronie's dirty hack for showing fanart slideshow instead of visualisation :)</description>
<posx>0</posx>
<posy>0</posy>
<width>720</width>
<height>576</height>
