			<posx>20</posx>
			<posy>115</posy>
			<width>350</width>
			<height>350</height>
			<info>ListItem.Icon</info>
			<aspectratio>keep</aspectratio>
			<visible>Control.IsVisible(50)</visible>
			<animation effect="fade" time="300">WindowClose</animation>
		</control>


