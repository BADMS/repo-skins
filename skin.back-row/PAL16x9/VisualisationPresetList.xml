<window>
	<id>122</id>
	<defaultcontrol>2</defaultcontrol>
	<include>dialog-effect</include> 
	<coordinates>
		<system>1</system>
		<origin x="150" y="90">true</origin>
	</coordinates>
	<controls>

		<control>
			<description>background image</description>
			<type>image</type>
			<id>0</id>
			<posx>0</posx>
			<posy>25</posy>
			<width>420</width>
			<height>320</height>
			<texture>osd_menu_bg.png</texture>
			<colordiffuse>BBffffff</colordiffuse>
		</control>

		<control>
			<description>background image</description>
			<type>image</type>
			<id>0</id>
			<posx>0</posx>
			<posy>-5</posy>
			<width>420</width>
			<height>31</height>
			<texture>osd_settings_header.png</texture>
			<colordiffuse>BBffffff</colordiffuse>
		</control>

		<control>
			<description>Presets Label</description>
			<type>label</type>
			<id>3</id>
			<posx>0</posx>
			<posy>2</posy>
			<width>420</width>
			<align>center</align>
			<font>font14</font>
		</control>

		<control>
			<description>No Presets Label</description>
			<type>label</type>
			<id>4</id>
			<posx>0</posx>
			<posy>120</posy>
			<width>420</width>
			<align>center</align>
			<label>13389</label>
		</control>


		<control type="list" id="2">
			<posx>10</posx>
			<posy>30</posy>
			<width>398</width>
			<height>300</height>
			<onleft>60</onleft>
			<onright>60</onright>
			<onup>2</onup>
			<ondown>2</ondown>
			<pagecontrol>60</pagecontrol>
			<scrolltime>200</scrolltime>

			<itemlayout height="30" width="398">

				<control type="label">
					<posx>10</posx>
					<posy>5</posy>
					<width>378</width>
					<height>20</height>
					<font>fontKB</font>
					<textcolor>grey</textcolor>
					<selectedcolor>white</selectedcolor>
					<align>left</align>
					<aligny>center</aligny>
					<label>$INFO[ListItem.Label]</label>
				</control>

			</itemlayout>

			<focusedlayout height="30" width="398">

				<control type="image">
					<posx>3</posx>
					<posy>0</posy>
					<width>392</width>
					<height>30</height>
					<texture>input-nofocus.png</texture>
					<visible>!Control.HasFocus(2)</visible>
					<include>VisibleFadeEffect</include>
				</control>

				<control type="image">
					<posx>3</posx>
					<posy>0</posy>
					<width>392</width>
					<height>30</height>
					<texture>input-focus.png</texture>
					<visible>Control.HasFocus(2)</visible>
					<include>VisibleFadeEffect</include>
				</control>

				<control type="label">
					<posx>10</posx>
					<posy>5</posy>
					<width>378</width>
					<height>20</height>
					<font>fontKB</font>
					<textcolor>lightgrey</textcolor>
					<selectedcolor>white</selectedcolor>
					<align>left</align>
					<aligny>center</aligny>
					<label>$INFO[ListItem.Label]</label>
				</control>

			</focusedlayout>

		</control>

		<control type="scrollbar" id="60">
			<posx>5</posx>
			<posy>30</posy>
			<width>4</width>
			<height>300</height>				
			<texturesliderbackground border="0,9,0,9">scrollbar_background.png</texturesliderbackground>
			<texturesliderbar border="0,9,0,9">scrollbar_background.png</texturesliderbar>
			<texturesliderbarfocus border="0,9,0,9">scrollbar_background.png</texturesliderbarfocus>
			<textureslidernib>scrollbar_nib.png</textureslidernib>
			<textureslidernibfocus>scrollbar_nib_focus.png</textureslidernibfocus>
			<onleft>2</onleft>
			<onright>2</onright>
			<ondown>60</ondown>
			<onup>60</onup>
			<showonepage>false</showonepage>
			<orientation>vertical</orientation>
			<colordiffuse>DDffffff</colordiffuse>
		</control>

	</controls>
</window>