<window>
<id>2000</id>
<defaultcontrol>3</defaultcontrol>
<allowoverlay>yes</allowoverlay>
<include>dialog-effect</include> 
<controls>

<control type="group">
<animation effect="slide" time="600" start="650,0" end="0,0" acceleration="-1" condition="!Skin.HasSetting(animated)">WindowOpen</animation>
<animation effect="slide" time="600" start="0,0" end="730,0" acceleration="-1" condition="!Skin.HasSetting(animated)">WindowClose</animation>

<control type="image">
<description>background</description>
<posx>-2</posx>
<posy>0</posy>
<width>726</width>
<height>578</height>
<texture>blackgloss.png</texture>
<aspectratio>stretch</aspectratio>
</control>

<control>
<description>Profile Background</description>
<type>image</type>
<id>1</id>
<posx>15</posx>
<posy>75</posy>
<width>690</width>
<height>423</height>
<texture>background2.png</texture>
<bordertexture>white.png</bordertexture>
<bordersize>1,1,2,2</bordersize>
<aspectratio>stretch</aspectratio>
</control>

<control type="label" id="1">
<description>header label</description>
<posx>20</posx>
<posy>80</posy>
<width>700</width>
<height>30</height>
<font>special16</font>
<label>$LOCALIZE[13406]</label>
<align>left</align>
<aligny>center</aligny>
<textcolor>white</textcolor>
<shadowcolor>black</shadowcolor>
</control>


<control type="button" id="5">
<description>Manual button</description>
<posx>20</posx>
<posy>460</posy>
<height>30</height>
<width>100</width>
<align>center</align>
<texturefocus>button-focus.png</texturefocus>
<texturenofocus>button-nofocus.png</texturenofocus>
<font>font14</font>
<onleft>3</onleft>
<onright>3</onright>
<onup>3</onup>
<ondown>3</ondown>
</control>

<control>
<description>number of files text label</description>
<type>label</type>
<id>2</id>
<posx>695</posx>
<posy>475</posy>
<height>20</height>
<width>100</width>
<label>-</label>
<align>right</align>
<font>font14</font>
</control>

<control type="list" id="3">
<viewtype label="535">list</viewtype>
<posx>20</posx>
<posy>120</posy>
<width>400</width>
<height>330</height>
<onleft>5</onleft>
<onright>5</onright>
<onup>3</onup>
<ondown>3</ondown>
<pagecontrol>90</pagecontrol>

<itemlayout height="30">

<control type="label">
<posx>5</posx>
<posy></posy>
<width>390</width>
<height>30</height>
<font>font15</font>
<aligny>center</aligny>
<textcolor>white</textcolor>
<shadowcolor>black</shadowcolor>
<include>selectedcolor</include>
<info>ListItem.Label</info>
</control>

</itemlayout>

<focusedlayout height="30">

<control type="image">
<posx>0</posx>
<posy>0</posy>
<width>400</width>
<height>30</height>
<texture>button3.png</texture>
<colordiffuse>77ffffff</colordiffuse>
<visible>Control.HasFocus(3)</visible>
</control>

<control type="label">
<posx>5</posx>
<posy>0</posy>
<width>390</width>
<height>30</height>
<font>font15</font>
<aligny>center</aligny>
<textcolor>white</textcolor>
<shadowcolor>black</shadowcolor>
<include>selectedcolor</include>
<scroll>true</scroll>
<scrolltime>100</scrolltime>
<info>ListItem.Label</info>
</control>
</focusedlayout>

</control>

<control type="list" id="6">
<viewtype label="535">list</viewtype>
<posx>20</posx>
<posy>120</posy>
<width>360</width>
<height>330</height>
<onleft>5</onleft>
<onright>5</onright>
<onup>6</onup>
<ondown>6</ondown>
<pagecontrol>90</pagecontrol>

<itemlayout height="66">

<control type="image">
<posx>2.5</posx>
<posy>8</posy>
<width>60</width>
<height>50</height>
<texture>$INFO[Listitem.Icon]</texture>
<bordertexture border="5">background2.png</bordertexture>
<bordersize>2</bordersize>
<aspectratio align="left">keep</aspectratio> 
</control>

<control type="label">
<posx>70</posx>
<posy>13</posy>
<width>285</width>
<height>20</height>
<align>left</align>
<aligny>bottom</aligny>
<font>font18</font>
<textcolor>white</textcolor>
<shadowcolor>black</shadowcolor>
<shadowcolor>black</shadowcolor>
<label>$INFO[ListItem.Label]</label>
</control>

<control type="textbox">
<posx>70</posx>
<posy>33</posy>
<width>285</width>
<height>20</height>
<font>font13</font>
<align>left</align>
<textcolor>lightgrey</textcolor>
<shadowcolor>black</shadowcolor>
<scroll>true</scroll>
<label>$INFO[ListItem.Label2]</label>
</control>

</itemlayout>

<focusedlayout height="66">

<control type="image">
<posx>0</posx>
<posy>3</posy>
<width>360</width>
<height>60</height>
<texture>white.png</texture>
<bordertexture>background2.png</bordertexture>
<bordersize>0,2,0,2</bordersize>
</control>

<control type="image">
<posx>2.5</posx>
<posy>8</posy>
<width>60</width>
<height>50</height>
<texture>$INFO[Listitem.Icon]</texture>
<bordertexture border="5">background2.png</bordertexture>
<bordersize>2</bordersize>
<aspectratio align="left">keep</aspectratio> 
</control>

<control type="label">
<posx>70</posx>
<posy>13</posy>
<width>285</width>
<height>20</height>
<align>left</align>
<aligny>bottom</aligny>
<font>font18</font>
<textcolor>white</textcolor>
<shadowcolor>black</shadowcolor>
<shadowcolor>black</shadowcolor>
<label>$INFO[ListItem.Label]</label>
</control>

<control type="textbox">
<posx>70</posx>
<posy>33</posy>
<width>285</width>
<height>20</height>
<font>font13</font>
<align>left</align>
<textcolor>lightgrey</textcolor>
<shadowcolor>black</shadowcolor>
<scroll>true</scroll>
<label>$INFO[ListItem.Label2]</label>
</control>

</focusedlayout>

</control>


</control>

</controls>
</window>