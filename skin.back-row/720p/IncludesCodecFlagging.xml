		</control> 	
		<control type="image"> 	
			<description>Audio Channels Image</description> 	
			<width>107</width> 	
			<height>30.25</height> 	
			<aspectratio>keep</aspectratio> 	
			<texture>$INFO[ListItem.AudioChannels,special://skin/images/flagging/audio/,.png]</texture> 	
		</control> 	
		<control type="image"> 	
