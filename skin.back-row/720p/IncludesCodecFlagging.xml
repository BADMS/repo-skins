			<height>30.25</height> 	
			<aspectratio>keep</aspectratio> 	
			<texture>$INFO[ListItem.AudioCodec,special://skin/images/flagging/audio/,.png]</texture> 	
			<!-- Don't show if I want the Dolby Digital Logo --> 	
			<visible>![stringcompare(ListItem.AudioCodec,ac3) + stringcompare(ListItem.AudioChannels,6)]</visible> 	
		</control> 	
		<control type="image"> 	
			<description>Dolby Digital Image</description> 	
			<width>107</width> 	
			<height>30.25</height> 	
			<aspectratio>keep</aspectratio> 	
			<texture>special://skin/images/flagging/audio/dolbydigital.png</texture> 	
