<?xml version="1.0" encoding="utf-8"?>
<includes>
    <variable name="videocodec">
        <value condition="[substring(ListItem.VideoCodec,div,left) | stringcompare(ListItem.VideoCodec,dx50)]">divx</value>
        <value>$INFO[ListItem.VideoCodec]</value>
    </variable>
    <variable name="videocodecOSD">
        <value condition="[substring(VideoPlayer.VideoCodec,div,left) | stringcompare(VideoPlayer.VideoCodec,dx50)]">divx</value>
        <value>$INFO[VideoPlayer.VideoCodec]</value>
    </variable>
    <include name="Codecs">
        <control type="image">
            <description>Video rez Image</description>
            <width>120</width>
            <height>80</height>
            <aspectratio>keep</aspectratio>
            <texture>$INFO[ListItem.VideoResolution,badges/video/,.png]</texture>
        </control>
        <control type="image">
            <description>Codec Image</description>
            <width>120</width>
            <height>80</height>
            <aspectratio>keep</aspectratio>
            <texture>$VAR[videocodec,badges/video/,.png]</texture>
        </control>
        <control type="image">
            <description>Audio Codec Image</description>
            <width>120</width>
            <height>80</height>
            <aspectratio>keep</aspectratio>
            <texture>$INFO[ListItem.AudioChannels,badges/audio/,.png]</texture>
        </control>
        <control type="image">
            <description>Audio Codec Image</description>
            <width>120</width>
            <height>80</height>
            <aspectratio>keep</aspectratio>
            <texture>$INFO[ListItem.AudioCodec,badges/audio/,.png]</texture>
        </control>
        <control type="image">
            <description>MPAA Image</description>
            <width>120</width>
            <height>80</height>
            <aspectratio>keep</aspectratio>
            <texture>$VAR[MediaFlag_MPAA]</texture>
        </control>
        <control type="group" id="1">
            <width>120</width>
            <height>80</height>
            <visible>!IsEmpty(ListItem.Top250) + !SubString(ListItem.Top250,0,Left)</visible>
            <control type="image" id="1">
                <width>120</width>
                <height>80</height>
                <aspectratio>keep</aspectratio>
                <texture>badges/imdb.png</texture>
            </control>
            <control type="label" id="1">
                <posx>0</posx>
                <top>15</top>
                <width>120</width>
                <height>80</height>
                <font>BadgeFont</font>
                <align>center</align>
                <label>$INFO[ListItem.Top250]</label>
                <textcolor>AlternateBrightColor</textcolor>
            </control>
        </control>
        <control type="image">
            <visible>ListItem.IsStereoscopic</visible>
            <description>3D Image</description>
            <width>120</width>
            <height>80</height>
            <aspectratio>keep</aspectratio>
            <texture>badges/3D.png</texture>
        </control>
    </include>
