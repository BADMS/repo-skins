<window id="34">
  <defaultcontrol always="true">2</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <include>OpenClose</include>
  <controls>
    <include>MiniBackground</include>
    <control type="label" description="Heading">
      <include>HeadingR</include>
      <label>10034</label>
    </control>
    <control type="label">
      <include>DialogID</include>
      <label>SettingsProfile.xml</label>
    </control>
    <control type="togglebutton" id="4" description="Enable Login Screen">
      <posx>680</posx>
      <posy>94</posy>
      <height>54</height>
      <width>535</width>
      <label>$LOCALIZE[20096]:  $LOCALIZE[1223]</label>
      <altlabel>$LOCALIZE[20096]:  $LOCALIZE[305]</altlabel>
      <align>left</align>
      <onright>-</onright>
      <onleft>-</onleft>
      <onup>2</onup>
      <ondown>2</ondown>
      <font>Normal</font>
      <textoffsetx>20</textoffsetx>
      <texturenofocus border="5">img/SettingsButtonNF.png</texturenofocus>
      <texturefocus border="5">img/SettingsButtonFO.png</texturefocus>
      <alttexturenofocus border="5">img/SettingsButtonNF.png</alttexturenofocus>
      <alttexturefocus border="5">img/SettingsButtonFO.png</alttexturefocus>
    </control>
    <control type="image">
      <animation effect="fade" time="300">VisibleChange</animation>
      <posx>660</posx>
      <posy>171</posy>
      <width>598</width>
      <height>1</height>
      <texture>img/Line.png</texture>
      <aspectratio>stretch</aspectratio>
    </control>
    <control type="group" description="Slider and List">
      <posx>680</posx>
      <posy>188</posy>
      <control type="group" description="Slider">
        <control type="image">
          <visible>!Control.HasFocus(4)</visible>
          <animation effect="fade" time="300">VisibleChange</animation>
          <posx>-26</posx>
          <posy>249</posy>
          <width>598</width>
          <height>98</height>
          <texture border="110,0,110,0">img/Slider4.png</texture>
          <aspectratio>stretch</aspectratio>
        </control>
        <animation condition="Container(2).Position(0)" effect="slide" start="0,0" end="0,-270"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(2).Position(1)" effect="slide" start="0,0" end="0,-216"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(2).Position(2)" effect="slide" start="0,0" end="0,-162"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(2).Position(3)" effect="slide" start="0,0" end="0,-108"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(2).Position(4)" effect="slide" start="0,0" end="0,-54"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(2).Position(5)" effect="slide" start="0,0" end="0,0"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(2).Position(6)" effect="slide" start="0,0" end="0,54"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(2).Position(7)" effect="slide" start="0,0" end="0,108"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(2).Position(8)" effect="slide" start="0,0" end="0,162"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(2).Position(9)" effect="slide" start="0,0" end="0,216"
					time="180" easing="inout" reversible="true">Conditional</animation>
        <animation condition="Container(2).Position(10)" effect="slide" start="0,0" end="0,270"
					time="180" easing="inout" reversible="true">Conditional</animation>
      </control>
      <control type="panel" id="2">
        <posx>0</posx>
        <posy>0</posy>
        <width>535</width>
        <height>594</height>
        <onleft>-</onleft>
        <onright>-</onright>
        <onup>4</onup>
        <ondown>4</ondown>
        <pagecontrol>60</pagecontrol>
        <focusposition>5</focusposition>
        <movement>5</movement>
        <preloaditems>4</preloaditems>
        <scrolltime tween="sine" easing="inout">280</scrolltime>
        <itemlayout width="535" height="54">
          <control type="label" description="Main Label">
            <visible>!StringCompare(ListItem.Label,..)</visible>
            <include>52MainLabel</include>
            <scroll>false</scroll>
            <textcolor>White</textcolor>
            <label>$INFO[ListItem.Label]</label>
          </control>
          <control type="label" description="Label2">
            <width>300</width>
            <include>52Label2</include>
            <scroll>false</scroll>
            <textcolor>Grey</textcolor>
            <label>$INFO[ListItem.Label2]</label>
          </control>
        </itemlayout>
        <focusedlayout width="535" height="54">
          <control type="label" description="Main Label">
            <visible>!StringCompare(ListItem.Label,..)</visible>
            <include>52MainLabel</include>
            <scroll>false</scroll>
            <textcolor>White2</textcolor>
            <label>$INFO[ListItem.Label]</label>
          </control>
          <control type="label" description="Label2">
            <width>300</width>
            <include>52Label2</include>
            <scroll>false</scroll>
            <textcolor>Grey</textcolor>
            <label>$INFO[ListItem.Label2]</label>
          </control>
        </focusedlayout>
      </control>
    </control>
    <include>Debug_Grid</include>
  </controls>
</window>
